{
    "interface_version": 2,
    "name": "EstateAutoAssistant3",
    "description": "第三代网页填报助手",
    "contact": "natsukawa247@outlook.com",
    "license": "MIT",
    "welcome": "首次使用！欢迎使用 EstateAutoAssistant3!",
    "github": "https://github.com/NtskwK/EAA3",
    "version": "1.0.0",
    "controller": [
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "青柠起始页",
                // 截图及控制方式，请根据项目实际情况修改
                // 文档: https://maafw.xyz/docs/2.4-ControlMethods
                "screencap": "PrintWindow", // 截图方式
                "mouse": "SendMessage", // 鼠标控制方式
                "keyboard": "SendMessage" // 键盘控制方式
            }
        }
    ],
    "resource": [
        {
            "name": "默认资源",
            "path": [
                "./resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "../.venv/Scripts/python.exe",
        "child_args": [
            "-u",
            "../agent/main.py"
        ],
        "identifier": "3be2c931-9c0d-4d69-9ebd-eed2263e06fd"
    },
    "task": [
        {
            "name": "选择数据",
            "entry": "SelectDatasetRow",
            "description": "选择数据行。<span style=\"color:tomato\">必选！</span>",
            "default_check": true
        },
        {
            "name": "首次宗地调查",
            "entry": "FirstTimeEstateSurvey",
            "default_check": true
        },
        {
            "name": "首次实测幢调查",
            "entry": "FirstTimeSCZSurvey",
            "default_check": true
        },
        {
            "name": "登簿",
            "entry": "Register",
            "default_check": true
        }
    ]
}
